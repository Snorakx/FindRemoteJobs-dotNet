@model JobsListModel

@{
    ViewBag.Title = "Index";
}

<h1>Jobs list</h1>

<div class="row">
    <div>
        <table class="table table-sm table-striped table-bordered m-2">
            <tbody>
                @foreach (var r in Model.Jobs)
                {
                    <tr>

                        <td>@r.Id</td>
                        <td>@r.Title</td>
                        <td>@r.Category</td>

                    </tr>
                }

            </tbody>
        </table>
    </div>
</div>